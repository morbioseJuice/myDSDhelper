{
    "manifest_version": 3,
    "name": "MyDSD Helper",
    "description": "A extension to give more detail on grades in MyDSD.",
    "version": "1.0",
    "action": {
        "default_popup": "mydsd.html"
    },
    "content_scripts": [
        {
            "js": ["script.js"],
            "matches": [
                "https://mydsd.davis.k12.ut.us/Student/Class/*"
            ]
        }
    ],
    "icons": {
        "16": "16.png",
        "32": "32.png",
        "48": "48.png",
        "128": "128.png"
    }
}