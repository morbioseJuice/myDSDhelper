{
    "manifest_version": 3,
    "name": "MyDSD Helper",
    "description": "Base Level Extension",
    "version": "1.0",
    "action": {
      "default_popup": "mydsd.html",
      "default_icon": "mydsd.html"
    },
    "content_scripts": [
        {
            "js": ["script.js"],
            "matches": [
                "https://mydsd.davis.k12.ut.us/Student/Class/*"
            ]
        }
    ]
}